<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>adicionarNovaResenhaBean</managed-bean-name>
		<managed-bean-class>comandos.AdicionarNovaResenhaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>criarLoginBean</managed-bean-name>
		<managed-bean-class>comandos.CriarLoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>criarNovoTopicoForumBean</managed-bean-name>
		<managed-bean-class>comandos.CriarNovoTopicoForumBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>efetuarLoginBean</managed-bean-name>
		<managed-bean-class>comandos.EfetuarLoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>verForumBean</managed-bean-name>
		<managed-bean-class>comandos.VerForumBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>verResenhasBean</managed-bean-name>
		<managed-bean-class>comandos.VerResenhasBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>visualizarTopicoDeForumBean</managed-bean-name>
		<managed-bean-class>comandos.VisualizarTopicoDeForumBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Forum.xhtml</display-name>
		<from-view-id>/Forum.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>realizouVisualizarTopicoDeForum</from-outcome>
			<to-view-id>/VisualizacaoTopicoDeForum.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>criarnovaresenha.xhtml</display-name>
		<from-view-id>/criarnovaresenha.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>completouVerForum</from-outcome>
			<to-view-id>/Forum.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>criarnovaresenha.xhtml</display-name>
		<from-view-id>/criarnovaresenha.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucessPegarTitulosENotasDeTodasAsResenhas</from-outcome>
			<to-view-id>/resenhas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>criarnovaresenha.xhtml</display-name>
		<from-view-id>/criarnovaresenha.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>successAdicionarNovaResenha</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>criarTopico.xhtml</display-name>
		<from-view-id>/criarTopico.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucessPegarTitulosENotasDeTodasAsResenhas</from-outcome>
			<to-view-id>/resenhas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>criarTopico.xhtml</display-name>
		<from-view-id>/criarTopico.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>completouVerForum</from-outcome>
			<to-view-id>/Forum.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>criarTopico.xhtml</display-name>
		<from-view-id>/criarTopico.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>completouCriarNovoTopicoForum</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>criarPaginaLivro.xhtml</display-name>
		<from-view-id>/criarPaginaLivro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>successCriarLogin</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucessPegarTitulosENotasDeTodasAsResenhas</from-outcome>
			<to-view-id>/resenhas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>completouVerForum</from-outcome>
			<to-view-id>/Forum.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>resenhas.xhtml</display-name>
		<from-view-id>/resenhas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>completouVerForum</from-outcome>
			<to-view-id>/Forum.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Forum.xhtml</display-name>
		<from-view-id>/Forum.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucessPegarTitulosENotasDeTodasAsResenhas</from-outcome>
			<to-view-id>/resenhas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<validator>
		<validator-id>validatorCampoPossuiPalavrao</validator-id>
		<validator-class>validadores.ValidadorCampoPossuiPalavrao</validator-class>
	</validator>
	<validator>
		<validator-id>validadorSenhaEstaCorreta</validator-id>
		<validator-class>validadores.ValidadorSenhaEstaCorreta</validator-class>
	</validator>

</faces-config>
